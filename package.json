{
  "name": "react-starting-point",
  "version": "1.0.0",
  "description": "Starting point for React, Redux and GraphQL projects",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start:gql": "babel-node tools/graphqlServer.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch start:gql",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Piero Divasto",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^1.1.2",
    "babel-polyfill": "6.26.0",
    "body-parser": "^1.18.2",
    "bootstrap": "3.3.7",
    "express": "^4.15.5",
    "graphql": "^0.11.7",
    "graphql-tools": "^2.0.0",
    "jquery": "3.2.1",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-redux": "4.0.8",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.26.0",
    "cheerio": "1.0.0-rc.2",
    "colors": "1.1.2",
    "compression": "1.7.0",
    "cross-env": "5.0.5",
    "css-loader": "0.28.7",
    "enzyme": "2.9.1",
    "eslint": "4.7.2",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-watch": "3.1.2",
    "eventsource-polyfill": "0.9.6",
    "expect": "21.1.0",
    "express": "4.15.5",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "jsdom": "11.2.0",
    "mocha": "3.5.3",
    "nock": "9.0.21",
    "npm-run-all": "4.1.1",
    "open": "0.0.5",
    "react-addons-test-utils": "15.6.0",
    "redux-immutable-state-invariant": "2.0.0",
    "redux-mock-store": "1.3.0",
    "rimraf": "2.6.2",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.6.0",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.19.1"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.nisumlatam.com:pdivasto/react-starting-project.git"
  }
}
